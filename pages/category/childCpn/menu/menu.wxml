<!--pages/category/childCpn/menu.wxml-->
<scroll-view  class="menu" scroll-y>
  <block wx:for="{{menuList}}" wx:key="index">
    <view class="category-item {{currentIndex==index ? 'active' : ''}}" bindtap = "itemClick" data-index = "{{index}}">
    {{item.title}}
    </view>
  </block>
</scroll-view> 
